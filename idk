wait(12)
for i = 1, 9 do
wait(.1)
gtrace.Transparency = gtrace.Transparency + .1
end
gtrace:Destroy()
end)
gdisp()
end
	local SFXZ = Instance.new("Sound",hit)
			SFXZ.SoundId = "rbxassetid://539294959"
