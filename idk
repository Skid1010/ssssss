    Waist.C0 = Waist.C0:lerp(CFrame.new( Waist.C0.p)*CFrame.new(0,0,0)*CFrame.Angles(-0.3,1,0),.1)--Head
		game:GetService("RunService").RenderStepped:wait()
		end
		KickBoom()
