				local ry,ht,ps=nil,nil,nil
while ht==nil do
	ry,ht,ps=newRay(hit.CFrame*CFrame.new(0,-1,0),hit.CFrame*CFrame.new(0,-2,0),4.1,{hit})
	wait()
end
for i = 1, 15 do
