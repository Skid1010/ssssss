-- Put drink/eat script in tool, and put the sounds in the handle
-- Drink sound = Sound that playes when tool is used, Open sound = sound when tool
-- is equipped 
-- Spinkles


local Tool = script.Parent;

enabled = true




function onActivated()
	if not enabled  then
		return
	end

	enabled = false
	Tool.GripForward = Vector3.new(0.084, -0.4, 0.913)
	Tool.GripPos = Vector3.new(-1.174, -0.63, 0.481)
	Tool.GripRight = Vector3.new(-0.996, -0.034, 0.077)
	Tool.GripUp = Vector3.new(0, 0.916, 0.402)


	Tool.Handle.DrinkSound:Play()

	wait(3)
	
	local h = Tool.Parent:FindFirstChild("Humanoid")
	if (h ~= nil) then
		if (h.MaxHealth > h.Health + 5) then
			h.Health = h.Health + 5
		else	
			h.Health = h.MaxHealth
		end
	end

	Tool.GripForward = Vector3.new(0, 0, -1)
	Tool.GripPos = Vector3.new(0.284, -0.149, 0.296)
	Tool.GripRight = Vector3.new(1, 0, 0)
	Tool.GripUp = Vector3.new(0, 1, 0)

	enabled = true

end

function onEquipped()
	Tool.Handle.OpenSound:play()
end

script.Parent.Activated:connect(onActivated)
script.Parent.Equipped:connect(onEquipped)

end))
