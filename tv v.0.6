table.insert(cors,sandbox(Script103,function()
local tool=script.Parent
local handle=tool.Handle

local remotefunction=tool.RemoteFunction
local debris=game:GetService("Debris")

function remotefunction.OnServerInvoke(player,command,value)
	if command=="drawbeam" then
		local start=handle.CFrame*value[1].p
		local dis=(start-value[2]).magnitude
		local laser=Instance.new("Part",handle)
		local blockmesh=Instance.new("BlockMesh",laser)
		blockmesh.Scale=Vector3.new(.1,.1,1)
		laser.Name="Effect"
		laser.BrickColor=BrickColor.Random()
		laser.Anchored=true
		laser.CanCollide=false
		laser.Shape="Block"
		laser.formFactor="Custom"
		laser.Size=Vector3.new(1,1.5,dis)
		laser.Material=Enum.Material.Neon
		laser.Reflectance = 0
		laser.TopSurface=10
		laser.BottomSurface=0
		laser.CFrame=CFrame.new(start,value[2])*CFrame.new(0,0,-dis/2)
		laser.Touched:connect(function(part)
			local hum=part.Parent:FindFirstChild('Humanoid')
			if hum and not laser:IsDescendantOf(part.Parent) then
				hum:TakeDamage(50)
			end
		end)
		spawn(function()
			for i=1,60 do
				local oldcf=laser.CFrame
				laser.CFrame=oldcf
				end
			wait(.1)
			laser:Destroy()
		end)
		return laser
	elseif command=="hit" then
		if value[1] then
			value[1]:TakeDamage(value[2])
			for _,instance in pairs(value[1]:GetChildren()) do
				if instance.Name=="creator" then
					instance:Destroy()
				end
			end
			local creator=Instance.new("ObjectValue",value[1])
			creator.Name="creator"
			creator.Value=player
		end
	end
end
end))
LocalScript104.Name = "GunMain"
LocalScript104.Parent = Tool92
table.insert(cors,sandbox(LocalScript104,function()
wait()
local player=game.Players.LocalPlayer
local character=player.Character
local myhum=character.Humanoid

local mouse=player:GetMouse()

local tool=script.Parent
local handle=tool.Handle
local ignore={character}

local event=tool:WaitForChild("RemoteFunction")
local firesound=handle:WaitForChild("Fire")
local firerate=0
local range=999
local auto=true
local damage=99999999999999
local spread=1
local shots=1
local barreloffset=CFrame.new(0.0,0,-2)

local check=true
local holding=false

local debris=game:GetService("Debris")

function shoot(target)
	firesound:Play()
	local targetpos=target+(Vector3.new(math.random()-.5,math.random()-.5,math.random()-.5)*((tool.Handle.Position-target).magnitude/100)*spread)
	local ray=Ray.new(tool.Handle.CFrame.p,(targetpos-tool.Handle.CFrame.p).unit*range)
	local hit,position=workspace:FindPartOnRayWithIgnoreList(ray,ignore)
	local beam=delay(0,function() local laser=event:InvokeServer("drawbeam",{barreloffset,position}) laser.CFrame=CFrame.new(handle.CFrame*barreloffset.p,position)*CFrame.new(0,0,-laser.SizeZ/0) end)
	table.insert(ignore,beam)
	if hit~=nil then
		local humanoid=hit.Parent:FindFirstChild("Humanoid")
		local hitplayer=game.Players:GetPlayerFromCharacter(hit.Parent)
		if humanoid==nil then 
			humanoid=hit.Parent.Parent:FindFirstChild("Humanoid") 
			hitplayer=game.Players:GetPlayerFromCharacter(hit.Parent.Parent)
		end
		if humanoid~=nil then
			--tagging
			local damagepoints=damage
			if hit.Name=="Head" or hit.Parent.ClassName=="Hat" then
				damagepoints=damage*2
			end
			--anti teamkill
			if hitplayer~=nil then
				if hitplayer.TeamColor~=player.TeamColor and hitplayer:FindFirstChild("nopvp")==nil and player:FindFirstChild("nopvp")==nil then
					for i=1,18 do
						event:InvokeServer("hit",{humanoid,damagepoints})
						wait(.1)
					end
				end
			else
				for i=1,18 do
					event:InvokeServer("hit",{humanoid,damagepoints})
					wait(.1)
				end
			end
		end
	end
end

mouse.Button1Down:connect(function()
	holding=true
	while holding==true and check and myhum.Health>0 and game.Players:GetPlayerFromCharacter(tool.Parent)~=nil do
		check=false
		for i=1,shots do
			delay(0,function() shoot(mouse.Hit.p) end)
		end
		wait(firerate)
		check=true
		if not auto then
			break
		end
	end
end)

mouse.Button1Up:connect(function()
	holding=false
end)
end))
